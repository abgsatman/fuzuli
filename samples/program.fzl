(require "/usr/lib/fuzuli/nfl/fltk.nfl")

(function button_event (params event)
	(block
		(print "Button event:\n")
		(print event "\n")
	)
)


(let form1 (form_new 300 300 "Hello Fuzuli GUI"))

(form_begin form1)

(let but1 (button_new 20 20 200 50 "Click here"))
(let but2 (button_new 20 80 200 50 "Do not click here"))


(form_add form1 but1)
(form_add form1 but2)

(form_end form1)

(form_show form1)
(main_loop)
