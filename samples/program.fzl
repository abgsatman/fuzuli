(class Person extends Object
   (block
      (def name STRING)
      (def age INTEGER)

      (function setName (params n)
            (block
                (let name n)
            )
       )

       (function getName (params)
         (block
            (return name)
         )
      )
   )
)

(let fuzuli (new Person))
(fuzuli.setName "FUZULI")
(print (fuzuli.getName))