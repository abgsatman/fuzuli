(function max (params a b)
	(block
		(if (> a b)
			(return a)
			(return b)
		)
	)
)

(function min (params a b)
	(block
		(if (< a b)
			(return a)
			(return b)
		)
	)
)

(function minmax (params a b c d) 
	(block
		(let min1 (min a b))
		(let min2 (min c d))
		(let result (max min1 min2))
		(return result)
	)
)

(function maxmin (params a b c d)
	(block
		(let max1 (max a b))
		(let max2 (max c d))
		(let result (min max1 max2))
		(return result)
	)
)


