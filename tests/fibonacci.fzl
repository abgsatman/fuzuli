(function fibonacci (params x)
    (block
    (if (<= x 2)
        (return 1)
        (return (+ (fibonacci (- x 1)) (fibonacci (- x 2))))
    )
    )
)


(if (= (fibonacci 3) 2)
	(print "PASSED!\n")
	(print "ERROR\n")
)

