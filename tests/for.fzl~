(let a 100)

# One For. 
(for (let i  0) (< i 10) (inc i)
		(inc a)
)

(if (= a 110) (print "PASSED!\n") (print "ERROR\n"))






# Two nested for's
(let toplam 0.0)
(for (let i 0) (< i 10) (inc i)
        (for (let j 0) (< j 10) (inc j)
                (inc toplam)
        )
)

(if (= toplam 100)
    (print "PASSED!\n")
    (print "ERROR\n")
)
                




# Three nested for's
(let toplam 0.0)
(for (let i 0) (< i 10) (inc i)
        (for (let j 0) (< j 10) (inc j)
                (for (let h 0) (< h 10) (inc h)
						(inc toplam)
				)
        )
)

(if (= toplam 1000)
    (print "PASSED!\n")
    (print "ERROR\n")
)





# for without block expression
(let i 0)
(for (let a 0) (< a 10) (inc a)
	(inc i)
)

(if (= i 10) (print "PASSED!\n") (print "ERROR\n"))

                      
      

