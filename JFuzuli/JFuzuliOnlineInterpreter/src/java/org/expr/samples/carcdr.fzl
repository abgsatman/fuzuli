(require "lang.nfl")

(let a (list 12 -9 6 1 20))
(let enter "<br>")

# First element
(print "car of a is " (car a) enter)

# Remaining part
(print "cdr of a is " (cdr a) enter)

# Third element
(print "car cdr cdr of a is " (car (cdr (cdr a))) enter)

# Nth element
(let i 2)
(print (+ i 1) "th element of a is " (nth a i) enter)

